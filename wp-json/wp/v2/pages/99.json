play:none;\r\n  }\r\n}\r\n\r\n.hamburger {\r\n  display:none; \r\n  flex-direction:column; \r\n  justify-content:space-between; \r\n  width:27.5px; \r\n  height:22px; \r\n  cursor:pointer;\r\n}\r\n.hamburger div {\r\n  height:3.3px; \r\n  background:var(--black); \r\n  border-radius:2.2px;\r\n}\r\n\r\n\/* By default, hide the close-menu link *\/\r\n.close-menu {\r\n  display:none; \r\n  text-transform:uppercase;\r\n  text-decoration:none;\r\n  color:var(--black);\r\n  text-align:center;\r\n  position:relative;\r\n}\r\n\r\n@media(max-width:768px) {\r\n  nav {\r\n    position:fixed;\r\n    top:66px;\r\n    left:0;\r\n    right:0;\r\n    bottom:0;\r\n    display:none;\r\n    flex-direction:column;\r\n    border-top:1.1px solid #eee;\r\n    z-index:9999;\r\n    overflow-y:auto;\r\n    background: var(--white);\r\n  }\r\n  nav.show { \r\n    display:flex; \r\n  }\r\n  nav a, .dropdown > a {\r\n    padding:16.5px;\r\n    border-bottom:1.1px solid #eee;\r\n    font-size:15.4px;\r\n    background: var(--white);\r\n  }\r\n  .hamburger { \r\n    display:flex; \r\n  }\r\n\r\n  .dropdown.active .dropdown-content {\r\n    display:block;\r\n    background: var(--white);\r\n    position:static;\r\n  }\r\n  .dropdown.active .arrow {\r\n    transform:rotate(180deg);\r\n  }\r\n\r\n  \/* Show the close-menu 'X' only on mobile *\/\r\n  .close-menu {\r\n    display:block;\r\n    padding:16.5px;\r\n    border-bottom:1.1px solid #eee;\r\n    font-size:15.4px;\r\n    background: var(--white);\r\n  }\r\n  .close-menu:hover::after {\r\n    content:\"\";\r\n    position:absolute; \r\n    left:0; \r\n    bottom:-2.2px; \r\n    width:100%; \r\n    height:2.2px; \r\n    background:var(--gold);\r\n  }\r\n}\r\n\r\n.dropdown-content a {\r\n  display:block;\r\n  padding:8.8px 22px;\r\n  white-space:nowrap;\r\n  color:var(--black);\r\n  text-transform:none;\r\n  font-weight:700;\r\n  font-size:15.4px;\r\n  background: var(--white);\r\n  cursor:pointer;\r\n}\r\n.dropdown-content a:hover {\r\n  backgro