= document.querySelectorAll('.dropdown');\r\n  dropdowns.forEach(function(dropdown) {\r\n    if (!dropdown.contains(event.target)) {\r\n      dropdown.classList.remove('active');\r\n    }\r\n  });\r\n});\r\n<\/script>\r\n<\/body>\r\n<\/html>\r\n\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/section>\n\t\t\t\t<section class=\"elementor-section elementor-top-section elementor-element elementor-element-13b4ac4b elementor-section-boxed elementor-section-height-default elementor-section-height-default\" data-id=\"13b4ac4b\" data-element_type=\"section\" data-settings=\"{&quot;background_background&quot;:&quot;classic&quot;}\">\n\t\t\t\t\t\t<div class=\"elementor-container elementor-column-gap-default\">\n\t\t\t\t\t<div class=\"elementor-column elementor-col-100 elementor-top-column elementor-element elementor-element-4fd57440\" data-id=\"4fd57440\" data-element_type=\"column\">\n\t\t\t<div class=\"elementor-widget-wrap elementor-element-populated\">\n\t\t\t\t\t\t<div class=\"elementor-element elementor-element-24174ab3 elementor-widget elementor-widget-spacer\" data-id=\"24174ab3\" data-element_type=\"widget\" data-widget_type=\"spacer.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t<div class=\"elementor-spacer\">\n\t\t\t<div class=\"elementor-spacer-inner\"><\/div>\n\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-1ed90709 elementor-widget elementor-widget-spacer\" data-id=\"1ed90709\" data-element_type=\"widget\" data-widget_type=\"spacer.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t<div class=\"elementor-spacer\">\n\t\t\t<div class=\"elementor-spacer-inner\"><\/div>\n\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-47890b1f elementor-widget elementor-widget-heading\" data-id=\"47890b1f\" data-element_type=\"widget\" data-widget_type=\"heading.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t<h2 class=\"elementor-heading-title elementor-size-default\">Microdermabrasie<\/h2>\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-5518114a elementor-widget elementor-widget-spacer\" data-id=\"5518114a\" data-element_type=\"widget\" data-widget_type=\"spacer.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t<div class=\"elementor-spacer\">\n\t\t\t<div class=\"elementor-spacer-inner\"><\/div>\n\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/section>\n\t\t\t\t<section class=\"elementor-section elementor-top-section elementor-element elementor-element-3314ec42 elementor-section-boxed elementor-section-height-default elementor-section-height-default\" data-id=\"3314ec42\" data-element_type=\"section\" data-settings=\"{&quot;background_background&quot;:&quot;classic&quot;}\">\n\t\t\t\t\t\t<div class=\"elementor-container elementor-column-gap-default\">\n\t\t\t\t\t<div class=\"elementor-column elementor-col-100 elementor-top-column elementor-element elementor-element-2551e5ab\" data-id=\"2551e5ab\" data-element_type=\"column\">\n\t\t\t<div class=\"elementor-widget-wrap elementor-element-populated\">\n\t\t\t\t\t\t<div class=\"elementor-element elementor-element-10d0c13a elementor-widget elementor-widget-spacer\" data-id=\"10d0c13a\" data-element_type=\"widget\" data-widget_type=\"spacer.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t<div class=\"elementor-spacer\">\n\t\t\t<div class=\"elementor-spacer-inner\"><\/div>\n\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/section>\n\t\t\t\t<section class=\"elementor-section elementor-top-section elementor-element elementor-element-7e2fa54e elementor-section-boxed elementor-section-height-default elementor-section-height-default\" data-id=\"7e2fa54e\" data-element_type=\"section\">\n\t\t\t\t\t\t<div class=\"elementor-container elementor-column-gap-default\">\n\t\t\t\t\t<div class=\"elementor-column elementor-col-100 elementor-top-column elementor-element elementor-element-72ad706a\" data-id=\"72ad706a\" data-element_type=\"column\">\n\t\t\t<div class=\"elementor-widget-wrap elementor-element-populated\">\n\t\t\t\t\t\t<div class=\"elementor-element elementor-element-cdd5ba5 elementor-widget elementor-widget-html\" data-id=\"cdd5ba5\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t<!-- Begin van de Microdermabrasie Tab -->\r\n<div class=\"beauty-tabs-container\">\r\n\r\n    <h1><\/h1>\r\n\r\n    <!-- Tab: Microdermabrasie -->\r\n    <div class=\"beauty-tab active\"> <!-- Klasse 'active' toegevoegd -->\r\n        <div class=\"beauty-tab-header\">Microdermabrasie<span class=\"tab-icon\">+<\/span><\/div>\r\n        <div class=\"beauty-tab-content\">\r\n            <p><strong>MICRODERMABRASIE Behandeling<\/strong><\/p>\r\n            <p><strong>Duur:<\/strong> 60 Minuten<\/p>\r\n            <p><strong>Prijs:<\/strong> \u20ac50.00<\/p>\r\n            <p>\r\n                MICRODERMABRASIE zorgt ervoor dat de aanwezige onzuiverheden zoals puistjes, mee-eters en gerstekorrels handmatig worden geledigd. Rimpeltjes, acn\u00e9 en pigmentverkleuringen kun je behandelen met onze microdermabrasie. In feite wordt de huid \"geslepen\", er wordt een zeer dun laagje huid verwijderd, waaronder een nieuwe, frisse huidlaag zit. Tevens wordt de doorbloeding door de zuigende werking gestimuleerd: de huid krijgt weer een gezonde tint door de gestimuleerde doorbloeding.\r\n            <\/p>\r\n        <\/div>\r\n    <\/div>\r\n\r\n<\/div>\r\n<!-- Einde van de Microdermabrasie Tab -->\r\n\r\n<!-- CSS Stijlen -->\r\n<style>\r\n    \/* Container Stijlen *\/\r\n    .beauty-tabs-container {\r\n        width: 100%;\r\n        max-width: 800px;\r\n        margin: 0 auto;\r\n        padding: 40px;\r\n        background-color: #f9f9f9; \/* Subtiele witte achtergrond *\/\r\n        border-radius: 10px;\r\n        box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);\r\n    }\r\n\r\n    .beauty-tabs-container h1 {\r\n        text-align: center;\r\n        color: #333333; \/* Zwarte koptekst *\/\r\n        margin-bottom: 30px;\r\n        font-size: 2.5em;\r\n        letter-spacing: 1px;\r\n    }\r\n\r\n    \/* Tab Stijlen *\/\r\n    .beauty-tab {\r\n        border: 1px solid #cccccc; \/* Lichtgrijze rand *\/\r\n        margin-bottom: 15px;\r\n        border-radius: 5px;\r\n        overflow: hidden;\r\n        background-color: #ffffff; \/* Witte tab achtergrond *\/\r\n        transition: background-color 0.3s ease;\r\n    }\r\n\r\n    .beauty-tab-header {\r\n        background-color: #e0e0e0; \/* Lichtgrijze header *\/\r\n        color: #000000; \/* Zwarte tekst *\/\r\n        padding: 15px 20px;\r\n        font-weight: bold;\r\n        cursor: pointer;\r\n        text-align: left;\r\n        font-size: 1.1em;\r\n        position: relative;\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: space-between;\r\n    }\r\n\r\n    .beauty-tab-header:hover {\r\n        background-color: #d5d5d5; \/* Iets donkerder grijs bij hover *\/\r\n    }\r\n\r\n    .tab-icon {\r\n        font-size: 1.2em;\r\n        transition: transform 0.3s ease;\r\n    }\r\n\r\n    .beauty-tab.active .tab-icon {\r\n        transform: rotate(45deg); \/* Draai '+' naar 'x' wanneer actief *\/\r\n    }\r\n\r\n    .beauty-tab-content {\r\n        padding: 20px;\r\n        background-color: #f5f5f5; \/* Zeer lichtgrijze achtergrond *\/\r\n        display: none;\r\n        font-size: 1em;\r\n        color: #333333; \/* Zwarte tekst *\/\r\n        border-top: 1px solid #cccccc; \/* Lichtgrijze rand boven *\/\r\n    }\r\n\r\n    .beauty-tab-content p {\r\n        margin: 10px 0;\r\n        line-height: 1.6;\r\n    }\r\n\r\n    .beauty-tab.active .beauty-tab-content {\r\n        display: block;\r\n    }\r\n\r\n    \/* Responsiviteit *\/\r\n    @media (max-width: 768px) {\r\n        .beauty-tabs-container {\r\n            padding: 20px;\r\n        }\r\n\r\n        .beauty-tabs-container h1 {\r\n            font-size: 2em;\r\n            margin-bottom: 20px;\r\n        }\r\n\r\n        .beauty-tab-header {\r\n            font-size: 1em;\r\n            padding: 12px 15px;\r\n        }\r\n\r\n        .beauty-tab-content {\r\n            padding: 15px;\r\n            font-size: 0.95em;\r\n        }\r\n    }\r\n<\/style>\r\n\r\n<!-- JavaScript voor Tab Functionaliteit -->\r\n<script>\r\n    document.addEventListener('DOMContentLoaded', function() {\r\n        const tabs = document.querySelectorAll('.beauty-tab-header');\r\n\r\n        tabs.forEach(tab => {\r\n            tab.addEventListener('click', function() {\r\n                const parentTab = this.parentElement;\r\n\r\n                \/\/ Toggle de actieve status van de geklikte tab\r\n                parentTab.classList.toggle('active');\r\n\r\n                \/\/ Optioneel: Sluit andere tabs wanneer een nieuwe tab wordt geopend\r\n                \/*\r\n                document.querySelectorAll('.beauty-tab').forEach(t => {\r\n                    if(t !== parentTab){\r\n                        t.classList.remove('active');\r\n                    }\r\n                });\r\n                *\/\r\n            });\r\n        });\r\n    });\r\n<\/script>\r\n\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/section>\n\t\t\t\t<section class=\"elementor-section elementor-top-section elementor-element elementor-element-7475dfd1 elementor-section-content-middle elementor-section-boxed elementor-section-height-default elementor-section-height-default\" data-id=\"7475dfd1\" data-element_type=\"section\" data-settings=\"{&quot;background_background&quot;:&quot;classic&quot;}\">\n\t\t\t\t\t\t<div class=\"elementor-container elementor-column-gap-default\">\n\t\t\t\t\t