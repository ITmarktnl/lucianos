ab-icon {\r\n        font-size: 1.2em;\r\n        transition: transform 0.3s ease;\r\n    }\r\n\r\n    .beauty-tab.active .tab-icon {\r\n        transform: rotate(45deg); \/* Draai '+' naar 'x' wanneer actief *\/\r\n    }\r\n\r\n    .beauty-tab-content {\r\n        padding: 20px;\r\n        background-color: #f5f5f5; \/* Zeer lichtgrijze achtergrond *\/\r\n        display: none;\r\n        font-size: 1em;\r\n        color: #333333; \/* Zwarte tekst *\/\r\n        border-top: 1px solid #cccccc; \/* Lichtgrijze rand boven *\/\r\n    }\r\n\r\n    .beauty-tab-content p {\r\n        margin: 10px 0;\r\n        line-height: 1.6;\r\n    }\r\n\r\n    .beauty-tab.active .beauty-tab-content {\r\n        display: block;\r\n    }\r\n\r\n    \/* Service Stijlen *\/\r\n    .service {\r\n        margin-bottom: 15px;\r\n    }\r\n\r\n    .service p {\r\n        margin: 5px 0;\r\n    }\r\n\r\n    hr {\r\n        border: none;\r\n        border-top: 1px solid #cccccc;\r\n        margin: 15px 0;\r\n    }\r\n\r\n    \/* Responsiviteit *\/\r\n    @media (max-width: 768px) {\r\n        .beauty-tabs-container {\r\n            padding: 20px;\r\n        }\r\n\r\n        .beauty-tabs-container h1 {\r\n            font-size: 2em;\r\n            margin-bottom: 20px;\r\n        }\r\n\r\n        .beauty-tab-header {\r\n            font-size: 1em;\r\n            padding: 12px 15px;\r\n        }\r\n\r\n        .beauty-tab-content {\r\n            padding: 15px;\r\n            font-size: 0.95em;\r\n        }\r\n    }\r\n<\/style>\r\n\r\n<!-- JavaScript voor Tab Functionaliteit -->\r\n<script>\r\n    document.addEventListener('DOMContentLoaded', function() {\r\n        const tabs = document.querySelectorAll('.beauty-tab-header');\r\n\r\n        tabs.forEach(tab => {\r\n            tab.addEventListener('click', function() {\r\n                const parentTab = this.parentElement;\r\n\r\n                \/\/ Toggle de actieve status van de geklikte tab\r\n                parentTab.classList.toggle('acti